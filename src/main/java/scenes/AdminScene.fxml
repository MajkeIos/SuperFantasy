<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminController">
   <children>
      <TextField fx:id="rosterCostField" layoutX="15.0" layoutY="88.0" prefHeight="25.0" prefWidth="141.0" />
      <TextField fx:id="gamesPathField" layoutX="15.0" layoutY="30.0" prefHeight="25.0" prefWidth="141.0" />
      <Button fx:id="getRostersPointsButton" layoutX="171.0" layoutY="18.0" mnemonicParsing="false" onAction="#getRostersPoints" prefHeight="50.0" prefWidth="115.0" text="Get rosters&#10;points" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="updateRosterCostButton" layoutX="171.0" layoutY="76.0" mnemonicParsing="false" onAction="#updateRostersCost" prefHeight="50.0" prefWidth="115.0" text="Update&#10;rosters cost" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="logoutButton" layoutX="93.0" layoutY="150.0" mnemonicParsing="false" onAction="#logout" prefHeight="36.0" prefWidth="115.0" text="LOGOUT" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
