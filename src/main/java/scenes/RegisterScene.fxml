<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RegisterController">
    <children>
        <Text layoutX="83.0" layoutY="150.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Text>
        <Text layoutX="80.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Text>
        <TextField fx:id="usernameField" layoutX="55.0" layoutY="80.0" prefHeight="20.0" prefWidth="200.0" />
        <Button fx:id="registerButton" layoutX="67.0" layoutY="310.0" mnemonicParsing="false" onAction="#register" prefWidth="175.0" text="Register">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Button>
        <Button fx:id="backButton" layoutX="21.0" layoutY="414.0" mnemonicParsing="false" onAction="#back" text="Back">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Button>
      <Text layoutX="17.0" layoutY="240.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Confirm Password:">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Text>
      <PasswordField fx:id="passwordField" layoutX="55.0" layoutY="170.0" prefHeight="20.0" prefWidth="200.0" />
      <PasswordField fx:id="confirmPasswordField" layoutX="55.0" layoutY="260.0" prefHeight="20.0" prefWidth="200.0" />
    </children>
</AnchorPane>
